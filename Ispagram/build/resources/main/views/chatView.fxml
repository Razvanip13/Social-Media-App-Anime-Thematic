<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="394.0" prefWidth="807.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.controller.ConversationsController">
   <children>
      <ListView fx:id="listViewChat" layoutX="226.0" layoutY="14.0" prefHeight="302.0" prefWidth="564.0" />
      <Button fx:id="buttonSendMessage" layoutX="719.0" layoutY="333.0" mnemonicParsing="false" onMouseClicked="#handleSendMessage" prefHeight="46.0" prefWidth="70.0" text="Send" />
      <TextArea fx:id="textAreaMessage" layoutX="359.0" layoutY="331.0" prefHeight="50.0" prefWidth="351.0" />
      <Button fx:id="buttonNext" layoutX="318.0" layoutY="333.0" mnemonicParsing="false" onMouseClicked="#handleNext" text="&gt;" />
      <Button fx:id="buttonBack" layoutX="269.0" layoutY="332.0" mnemonicParsing="false" onMouseClicked="#handleBack" text="&lt;" />
      <ImageView fx:id="imageViewChat" fitHeight="301.0" fitWidth="200.0" layoutX="14.0" layoutY="71.0" pickOnBounds="true" preserveRatio="true" />
      <Label fx:id="labelUserChat" layoutX="23.0" layoutY="42.0" prefHeight="19.0" prefWidth="190.0" text="Chat:">
         <font>
            <Font name="Lucida Bright" size="15.0" />
         </font>
      </Label>
      <Label fx:id="labelCountMessages" layoutX="232.0" layoutY="338.0" text="0/0" />
   </children>
</AnchorPane>
